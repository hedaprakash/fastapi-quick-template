
{
    "version": "0.2.0",
    "configurations": [
        {
          "name": "uv1",
          "type": "python",
          "request": "launch",
          "program": "${workspaceFolder}/main.py",
          "console": "integratedTerminal",
          "args": [
              "-m",
              "uvicorn",
              "main:app",
              "--host",
              "0.0.0.0",
              "--port",
              "80",
              "--reload"
          ],
          "env": {
              "PYTHONPATH": "${workspaceFolder}"
          }
        },
        {
            "name": "FastAPI-uvicorn",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "80",
                "--reload"
            ],
            "jinja": true,
            "console": "integratedTerminal"
        },
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "run",
                "uvicorn",
                "main:app",
                "--reload",
                "--host",
                "127.0.0.1",
                "--port",
                "8000"
            ],
            "jinja": true,
            "console": "integratedTerminal",
            "env": {
              "PYTHONPATH": "${workspaceFolder}/.venv/lib/python3.x/site-packages"
            }
          }
    ]
  }
  